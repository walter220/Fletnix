@model IEnumerable<TheWorld.Models.Genre>
@{
    ViewBag.Title = "Genre page";
}
<div class="page-header">
    <h1>Lijst van alle genre's</h1>
</div>
<div class="row">
    <div class="col-md-6">
        <h2>The genre's</h2>
        <ul>
            @foreach (var genre in Model)
            {
                <li>
                    @genre.genre_name
                </li>
            }
        </ul>
    </div>
</div>
